class Loader:
    def __init__(self, plugin: BasePlugin, activity: Activity):
        self.plugin = plugin
        self.activity = activity
        self.instance = None
        self.list_settings = ["DOT_SIZE_PERCENT",
                "STROKE_PERCENT",
                "OFFSET_ANGLE_DEGREES",
                "CHECK_INTERVAL_MS",
                "ANIM_DURATION_MS",
                "SHOW_COMMON_GROUPS"
                              ]

    def getInstance(self):
        if self.instance is None:
            try:
                method = find_class(CLASS_NAME).getClass().getMethod("getInstance")
                self.instance = method.invoke(None)
            except Exception as e:
                self.plugin.log(f"Error getting instance: {e}")
        return self.instance

    def apply_settings(self):
        try:
            for setting in self.list_settings:
                field = find_class(CLASS_NAME).getClass().getField(setting)
                setting_pl = self.plugin.get_setting(setting, -100)
                if setting_pl == -100:
                    return
                if setting in self.list_settings[:3]:
                    field.set(None, jfloat(float(setting_pl)))
                if setting in self.list_settings[3:5]:
                    field.set(None, int(setting_pl))
                if setting == "SHOW_COMMON_GROUPS":
                    b = self.plugin.get_setting(setting, True)
                    field.set(None, jboolean(bool(setting_pl)))
                self.plugin.log(f"Set {setting} to {setting_pl}")
        except Exception as e:
            self.plugin.log(f"Settings apply bad {e}")


    def open_settings(self):
        try:
            inst = self.getInstance()
            if inst is None:
                return
            inst.getClass().getMethod("showSettings").invoke(inst)
        except Exception as e:
            self.plugin.log(f"Error opening settings: {e}")

    def unload(self):
        try:
            inst = self.getInstance()
            if inst is None:
                return
            inst.getClass().getMethod("onUnload").invoke(inst)
            self.plugin.log("Success unload")
        except Exception as e:
            self.plugin.log(f"Error unload: {e}")

    def start_from_b64(self, b64: str):
        if not b64:
            raise ValueError("DEX_B64 is empty")

        # если вдруг строка всё же будет с переносами — это не страшно
        cleaned = "".join(b64.split())

        dex_bytes = base64.b64decode(cleaned)
        buffer = ByteBuffer.wrap(dex_bytes)

        dex_loader = InMemoryDexClassLoader(
            buffer,
            ApplicationLoader.applicationContext.getClassLoader()
        )

        clazz = dex_loader.loadClass(CLASS_NAME)
        instance = clazz.getMethod("getInstance").invoke(None)
        self.instance = instance

        clazz.getMethod(METHOD_NAME).invoke(instance)
        self.apply_settings()
        self.plugin.log(f"Loaded {CLASS_NAME} from Base64 and invoked {METHOD_NAME}")

    def load_and_start(self):
        self.start_from_b64(DEX_B64)


class Plugin(BasePlugin):
    def __init__(self):
        self.loader = None
        self.strings = {}

    def load_strings(self):
        if LocaleController.getInstance().getCurrentLocale().getLanguage() == "ru":
            self.strings = {
                "DOT_SIZE_PERCENT": "Размер точки",
                "STROKE_PERCENT": "Размер выреза",
                "OFFSET_ANGLE_DEGREES": "Расположение точки (градусы)",
                "CHECK_INTERVAL_MS": "Тайм-аут проверки статуса (мс)",
                "ANIM_DURATION_MS": "Длина анимации (мс)",
                "SHOW_COMMON_GROUPS": "Отображать наличие общих групп(кроме текущей)"
            }
        else:
            self.strings = {
                "DOT_SIZE_PERCENT": "Dot size",
                "STROKE_PERCENT": "Cutout size",
                "OFFSET_ANGLE_DEGREES": "Dot position (degrees)",
                "CHECK_INTERVAL_MS": "Status check interval (ms)",
                "ANIM_DURATION_MS": "Animation duration (ms)",
                "SHOW_COMMON_GROUPS": "Show the presence of common groups (except the current one)"
            }

    def on_plugin_load(self) -> None:
        try:
            self.load_strings()
            self.log(f"Init {__version__}")
            self.create_settings()
            self.load()
        except Exception as e:
            BulletinHelper.show_info(f"Error: {e}", get_last_fragment())
            self.log(f"Error: {e}")

    def create_settings(self) -> list[Any]:
        return [Input("DOT_SIZE_PERCENT", self.strings["DOT_SIZE_PERCENT"], default=0.18, on_change=lambda ignored: self.apply_settings()),
                Input("STROKE_PERCENT", self.strings["STROKE_PERCENT"], default=0.06, on_change=lambda ignored: self.apply_settings()),
                Input("OFFSET_ANGLE_DEGREES", self.strings["OFFSET_ANGLE_DEGREES"], default=45, on_change=lambda ignored: self.apply_settings()),
                Input("CHECK_INTERVAL_MS", self.strings["CHECK_INTERVAL_MS"], default=1000, on_change=lambda ignored: self.apply_settings()),
                Input("ANIM_DURATION_MS", self.strings["ANIM_DURATION_MS"], default=200, on_change=lambda ignored: self.apply_settings()),
                Switch("SHOW_COMMON_GROUPS", self.strings["SHOW_COMMON_GROUPS"], default=True, on_change=lambda ignored: self.apply_settings())
                ]
    def apply_settings(self):
        if self.loader is not None:
            self.loader.apply_settings()
    def on_plugin_unload(self) -> None:
        if self.loader is not None:
            self.loader.unload()

    def open_settings(self, context):
        if self.loader is not None:
            self.loader.open_settings()

    def load(self):
        launchActivity: LaunchActivity = get_last_fragment().getContext()
        self.loader = Loader(self, launchActivity)
        try:
            self.loader.load_and_start()
        except Exception as e:
            if "proxy" in str(e):
                return
            self.log(f"Fatal error: {e}")
            BulletinHelper.show_info(f"Error loading dex from Base64: {e}", get_last_fragment())
